extends ../layout/default

block content
  .row
    .col-md-8.col-md-offset-2

      h3 Materials
      table.table.table-striped.table-condensed
        tbody
          each item in items
            tr
              td
                .media
                  img.pull-left.media-object(src="http://image.eveonline.com/Type/#{item.typeID}_64.png")
                  .media-body
                    h5
                      a(href='/market/#{item.typeID}')= item.typeName
